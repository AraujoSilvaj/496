<launch>
    <!-- Launch node for the GPS -->
    <node pkg="atgm336h5n3x" exec="nmea_node" name="gps_node" output="screen"/>

<!-- IMU node
    <node pkg="y3space_driver" exec="Y3SpaceDriver" name="imu" output="screen"/> -->

    <!-- VESC driver node -->
    <node pkg="vesc_driver" exec="vesc_driver_node" name="vesc_driver" output="screen">
    <param name="port" value="/dev/ttyVESC" />
    </node>
</launch> 
